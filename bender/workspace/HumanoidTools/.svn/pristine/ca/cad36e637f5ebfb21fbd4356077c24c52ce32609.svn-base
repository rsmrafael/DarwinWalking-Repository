package de.fhwf.humanoid.tools.debugger.events;

import java.util.EventObject;

public class XYChangedEvent extends EventObject {
	private static final long serialVersionUID = 1L;

	private int mX;
	private int mY;
  
  public XYChangedEvent(Object source, int x, int y) { 
    super(source);
    mX = x;
    mY = y;
  } 

  public int getX() {
   return mX;
  }
  
  public int getY() {
   return mY;
  }
}