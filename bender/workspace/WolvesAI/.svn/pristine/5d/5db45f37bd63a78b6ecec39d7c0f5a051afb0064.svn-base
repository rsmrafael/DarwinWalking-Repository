#include "HistogramList.h"

HistogramList::HistogramList()
{

}

HistogramList::~HistogramList()
{

}

void HistogramList::clear()
{
	mData.clear();
}

void HistogramList::add(const Histogram& histogram)
{
	mData.push_back(histogram);
}

const vector<Histogram>& HistogramList::get() const
{
	return mData;
}
