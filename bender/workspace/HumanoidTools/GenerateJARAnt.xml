<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project HumanoidTools">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="Debugger.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="de.fhwf.humanoid.tools.debugger.Debugger"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="bin"/>
            <zipfileset excludes="META-INF/*.SF" src="lib/flexdock-0.5.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="lib/commons-logging-1.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="lib/jfreechart-1.0.13.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="lib/jcommon-1.0.16.jar"/>
        </jar>
    </target>
</project>
